        "# 台形の寸法\n",
        "top_side = 10\n",
        "bottom_side = 20\n",
        "height = 5\n",
        "\n",
        "# 台形の面積 =（上辺＋下辺）× 高さ ÷ 2\n",
        "area = (top_side + bottom_side) * height / 2\n",
        "\n",
        "# 面積結果と文字列cm²の連結\n",
        "print(f\"{area}cm²\")"
